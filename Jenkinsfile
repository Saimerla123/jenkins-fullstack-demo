pipeline {
    agent any

    stages {

        stage('Pull Code') {
            steps {
                echo 'Pulling code from GitHub...'
            }
        }

        stage('Build') {
            steps {
                echo 'Building project...'
            }
        }

        stage('Test') {
            steps {
                echo 'Running tests...'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying application...'
            }
        }
    }
}
